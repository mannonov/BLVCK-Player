<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="?colorAccent" />
            <size
                android:width="4dp"
                android:height="4dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <size
                android:width="4dp"
                android:height="4dp" />
            <solid android:color="?colorPrimaryDark" />
        </shape>
    </item>
</layer-list>